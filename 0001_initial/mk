
cat bootstrap.c bootstrappable.c support.c > ../../tmpdir/out.c

js js_to_c.js >> ../../tmpdir/out.c

cp ELF-i386.hex2 ../../tmpdir
cp libc-core.M1 ../../tmpdir
cp x86_defs.M1 ../../tmpdir

./mk_clean
cd ../../tmpdir/

cc_x86 out.c out_cc_x86.M1
cat x86_defs.M1 libc-core.M1 out_cc_x86.M1 > out-0.M1
M0 out-0.M1 out_-0.hex2
cat ELF-i386.hex2 out-0.hex2 > out-0-0.hex2
hex2-0 out-0-0.hex2 out.exe

